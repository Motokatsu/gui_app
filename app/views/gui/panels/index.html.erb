<h1>Listing panels</h1>

<table>
  <tr>
    <th>Module</th>
    <th>Name jp</th>
    <th>Name en</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @panels.each do |panel| %>
  <tr>
    <td><%= panel.module %></td>
    <td><%= panel.name_jp %></td>
    <td><%= panel.name_en %></td>
    <td><%= link_to 'Show', panel %></td>
    <td><%= link_to 'Edit', edit_gui_panel_path(panel) %></td>
    <td><%= link_to 'Destroy', panel, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Panel', new_gui_panel_path %>
